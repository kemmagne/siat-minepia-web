				<textFieldExpression><![CDATA["\u2022  <u>"+($F{designation} != null  && $F{designation}.trim().length()>0 ? $F{designation} : "") +"</u>:<b>"+($F{quanditeproposed} != 0.0  && $F{quanditeproposed} > 0 ? $F{quanditeproposed} : "")+"</b> KG"]]></textFieldExpression>
